{
  "name": "package-retriever",
  "version": "0.1.0",
  "description": "Tool suite to retrieve a package from either local cache, npm or git repo",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm test"
  },
  "keywords": [
    "templates",
    "retrieve",
    "download",
    "update",
    "cache"
  ],
  "author": "Kristian Mandrup <kmandrup@gmail.com>",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "deepmerge": "^2.0.1",
    "download-git-repo": "^1.0.2",
    "filenamify": "^2.0.0",
    "fs-extra": "^5.0.0",
    "recursive-readdir": "^2.2.2",
    "semver": "^5.5.0",
    "tildify": "^1.2.0",
    "user-home": "^2.0.0",
    "yarn-install": "^0.5.1",
    "update-notifier": "^2.3.0",
    "boxen": "^1.3.0",
    "find-derived": "github:kristianmandrup/find-derived"
  },
  "devDependencies": {
    "@types/deepmerge": "^1.3.3",
    "@types/filenamify": "^2.0.1",
    "@types/fs-extra": "^5.0.1",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "@types/recursive-readdir": "^2.2.0",
    "@types/semver": "^5.5.0",
    "@types/stringify-object": "^3.2.0",
    "@types/update-notifier": "^2.0.0",
    "@types/user-home": "^2.0.0",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2"
  }
}
